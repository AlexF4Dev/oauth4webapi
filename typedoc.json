{
  "$schema": "https://typedoc.org/schema.json",
  "disableSources": true,
  "parametersFormat": "table",
  "textContentMappings": {
    "title.memberPage": "{kind}: {name}\n\n[ðŸ’— Help the project](https://github.com/sponsors/panva)",
    "title.indexPage": "{projectName} API Reference"
  },
  "hideBreadcrumbs": true,
  "hidePageHeader": true,
  "entryPoints": ["src/index.ts"],
  "excludeExternals": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "gitRevision": "main",
  "hideGenerator": true,
  "out": "docs",
  "plugin": ["typedoc-plugin-markdown", "typedoc-plugin-mdn-links"],
  "readme": "none",
  "sort": [
    "kind",
    "static-first",
    "required-first",
    "alphabetical"
  ]
}
